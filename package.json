{
  "name": "wp-vimeo-slider",
  "version": "1.0.0",
  "description":
    "A horizontal slider playing Vimeo videos from a designated Vimeo account, with videos specified through the WordPress admin.",
  "main": "src/js",
  "author": "John Watkins <john.watkins@colby.edu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ColbyCommunications/wp-vimeo-slider"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "start:wp": "webpack -w --config webpack.build.config.babel.js",
    "build":
      "webpack --config webpack.build.config.babel.js && webpack -p --config webpack.build.config.babel.js && webpack --config webpack.build.config.babel.js && webpack -p --config webpack.build.config.babel.js",
    "lint": "eslint src/js --fix",
    "stylelint": "stylelint src/js",
    "test": "npm run lint && npm run stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run stylelint && composer phpcbf && composer lint"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "style-loader": "^0.20.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-processor-styled-components": "^1.2.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "dependencies": {
    "@vimeo/player": "^2.4.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-slick": "^0.18.1",
    "styled-components": "^2.4.0"
  }
}
